{
	"name": "v12mike/symbols",
	"type": "phpbb-extension",
	"description": "This extension allows users to embed symbols (HTML Entities) in posts and PMs.",
	"homepage": "https://github.com/v12mike/symbols",
	"version": "1.0.2-b1",
	"keywords": [
	        "phpbb",
	        "extension",
	        "symbols"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Mike Morrin (v12mike)",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0",
		"phpbb/phpbb": ">=3.2.0,<3.3"
	},
	"extra": {
		"display-name": "Symbols",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.2,<3.3"
		},
		"version-check": {
			"host": "frenchcarforum.co.uk",
			"directory": "/versions",
			"filename": "phpbb-symbols.json"
		}
	}
}
